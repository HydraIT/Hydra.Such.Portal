                                            </table>
                                        </template>
                                    </vaadin-combo-box>
                                </div>*@
                                <div class="col-lg-3">
                                    <vaadin-text-field value="{{LineDescription}}" maxlength="20" id="Descriptionid" name="DescriptionName" label="Descrição"> </vaadin-text-field>
                                </div>
                                <div class="col-lg-3">
                                    <vaadin-text-field value="{{LineQuantity}}" maxlength="20" pattern="^[0-9]\d*(\.\d+)?$" class="alignRight" id="Quantityid" name="QuantityName" label="Quantidade"> </vaadin-text-field>
