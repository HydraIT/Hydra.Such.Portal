                                        </div>
                                        <div class="col-md-3 ">
                                            <vaadin-combo-box items="[[CBWarehouses]]" id="warehouse" item-value-path="id" item-label-path="value" value="{{data.warehouse}}" label="Armazén" disabled="{{ !_permissions.update }}">
                                                <template>
                                                    <table width="180px">
                                                        <tr>
                                                            <td width="40%">[[item.id]]</td>
                                                            <td width="60%"> <b>[[item.value]]</b></td>
                                                        </tr>
                                                    </table>
                                                </template>
                                            </vaadin-combo-box>
                                        </div>
                                        <div class="col-md-3 ">
                                            <vaadin-combo-box items="[[CBWarehouses]]" id="warehouseSupplier" item-value-path="id" item-label-path="value" value="{{data.warehouseSupplier}}" label="Armazén Fornecedor" disabled="{{ !_permissions.update }}">
                                                <template>
                                                    <table width="180px">
                                                        <tr>
                                                            <td width="40%">[[item.id]]</td>
                                                            <td width="60%"> <b>[[item.value]]</b></td>
                                                        </tr>
                                                    </table>
                                                </template>
                                            </vaadin-combo-box>
                                        </div>
                                        <div class="col-md-3 ">
                                            <vaadin-combo-box items="[[CBRegions]]" id="mCodeRegion" item-value-path="id" item-label-path="value" value="{{data.codeRegion}}" label="Cód. Região" disabled="{{ !_permissions.update }}" >
                                                <template>
                                                    <table width="180px">
                                                        <tr>
                                                            <td width="40%">[[item.id]]</td>
                                                            <td width="60%"> <b>[[item.value]]</b></td>
                                                        </tr>
                                                    </table>
                                                </template>
                                            </vaadin-combo-box>
                                        </div>
                                    </div>
                                    <div class="row break_clear">
                                        <div class="col-md-3 ">
                                            <vaadin-combo-box items="[[CBFunctionalAreas]]" id="mFunctionalArea" item-value-path="id" item-label-path="value" value="{{data.codeFunctionalArea}}" label="Cód. Area Funcional" disabled="{{ !_permissions.update }}">
                                                <template>
                                                    <table width="180px">
                                                        <tr>
                                                            <td width="40%">[[item.id]]</td>
                                                            <td width="60%"> <b>[[item.value]]</b></td>
                                                        </tr>
                                                    </table>
                                                </template>
                                            </vaadin-combo-box>
                                        </div>
                                        <div class="col-md-3 ">
                                            <vaadin-combo-box items="[[CBResponsabilityCenters]]" id="mResponsabilityCenter" item-value-path="id" item-label-path="value" value="{{data.codeResponsabilityCenter}}" label="Cód. Centro Responsabilidade" disabled="{{ !_permissions.update }}">
                                                <template>
                                                    <table width="180px">
                                                        <tr>
                                                            <td width="40%">[[item.id]]</td>
                                                            <td width="60%"> <b>[[item.value]]</b></td>
                                                        </tr>
                                                    </table>
                                                </template>
                                            </vaadin-combo-box>
                                        </div>
                                        <div class="col-md-3 ">
                                            <vaadin-combo-box items="[[CBProjects]]" id="projectKitchen" item-value-path="id" item-label-path="value" value="{{data.projectKitchen}}" label="Projeto Cozinha" disabled="{{ !_permissions.update }}"  on-value-changed="_projectKitchenChange">
                                                <template>
                                                    <table width="180px">
                                                        <tr>
                                                            <td width="40%">[[item.id]]</td>
                                                            <td width="60%"> <b>[[item.value]]</b></td>
                                                        </tr>
                                                    </table>
                                                </template>
                                            </vaadin-combo-box>
                                        </div>
                                        <div class="col-md-3 ">
                                            <vaadin-text-field value="{{data.projectKitchenTotalMovs}}" id="projectKitchenTotalMovs" name="projectKitchenTotalMovs" label="Mov. Projeto Cozinha" disabled> </vaadin-text-field>
                                        </div>
                                    </div>

                                    <div class="row break_clear">
                                        <div class="col-md-3 ">
                                            <vaadin-combo-box items="[[CBProjects]]" id="projectWaste" item-value-path="id" item-label-path="value" value="{{data.projectWaste}}" label="Projeto Desperdicios" disabled="{{ !_permissions.update }}" on-value-changed="_projectWasteChange">
                                                <template>
                                                    <table width="180px">
                                                        <tr>
                                                            <td width="40%">[[item.id]]</td>
                                                            <td width="60%"> <b>[[item.value]]</b></td>
                                                        </tr>
                                                    </table>
                                                </template>
                                            </vaadin-combo-box>
                                        </div>
                                        <div class="col-md-3 ">
                                            <vaadin-text-field value="{{data.projectWasteTotalMovs}}" id="projectWasteTotalMovs" name="projectWasteTotalMovs" label="Mov. Projeto Desperdicios" disabled> </vaadin-text-field>
                                        </div>
                                        <div class="col-md-3 ">
                                            <vaadin-combo-box items="[[CBProjects]]" id="projectSubsidiaries" item-value-path="id" item-label-path="value" value="{{data.projectSubsidiaries}}" label="Projeto Mat. Subsidiárias" disabled="{{ !_permissions.update }}" on-value-changed="_projectSubsidiariesChange">
                                                <template>
                                                    <table width="180px">
                                                        <tr>
                                                            <td width="40%">[[item.id]]</td>
                                                            <td width="60%"> <b>[[item.value]]</b></td>
                                                        </tr>
                                                    </table>
                                                </template>
                                            </vaadin-combo-box>
                                        </div>
                                        <div class="col-md-3 ">
                                            <vaadin-text-field value="{{data.projectSubsidiariesTotalMovs}}" id="projectSubsidiariesTotalMovs" name="projectSubsidiariesTotalMovs" label="Mov. Projeto Subsidiárias" disabled> </vaadin-text-field>
                                        </div>
                                    </div>
                                </div>




                                <!-- Projetos de Faturação-->
                                <div>
                                    <div class="row break_clear">
                                        <div class="panel panel-default">
                                            <div class="panel-heading">Projetos de Faturação</div>
                                            <div class="panel-body">
                                                    <div class="row">
                                                        <div class="col-lg-3">
                                                            <vaadin-combo-box items="[[CBProjects]]" id="BPProjectNo" name="BPProjectNo" item-value-path="id" item-label-path="value" label="Projeto" disabled="{{ !_permissions.update }}" on-value-changed="_getProjectBillingData">
                                                                <template>
                                                                    <paper-icon-item>
                                                                        <paper-item-body two-line>
                                                                            <div>[[item.value]]</div>
                                                                        </paper-item-body>
                                                                    </paper-icon-item>
                                                                </template>
                                                            </vaadin-combo-box>
                                                        </div>
                                                        <div class="col-lg-3">
                                                            <vaadin-text-field id="BPClientNo" name="BPClientNo" label="Cód. Cliente" disabled> </vaadin-text-field>
                                                        </div>
                                                        <div class="col-lg-3">
                                                            <vaadin-text-field id="BPClientName" name="BPClientName" label="Cliente" disabled> </vaadin-text-field>
                                                        </div>
                                                        <div class="col-lg-2">
                                                            <vaadin-text-field id="BPTotalSales" name="BPTotalSales" label="Total de Vendas" disabled> </vaadin-text-field>
                                                        </div>
                                                        <div class="col-lg-1">
                                                            <paper-button class="btnSave" raised on-tap="_createBillingProject">Criar</paper-button>
                                                        </div>
                                                    </div>

                                                <!-- VAADIN GRID  Projetos de Faturação -->
                                                <vaadin-grid aria-label="BillingProjectsGrid" id="grid_BillingProjectsGrid" items="[[data.billingProjects]]" class="grid" multi-sort="true" disabled="{{ !_permissions.update }}">
                                                    <vaadin-grid-column>
                                                        <template class="header">
                                                            <vaadin-grid-sorter path="projectNo">Nº Projeto</vaadin-grid-sorter>
                                                            <vaadin-grid-filter path="projectNo" />
                                                        </template>
                                                        <template>
                                                            <vaadin-combo-box items="[[CBProjects]]" id="BPGridProjectNo" item-value-path="id" item-label-path="value" label="Projeto" value="{{item.projectNo}}" disabled="{{ !_permissions.update }}">
                                                                <template>
                                                                    <table width="180px">
                                                                        <tr>
                                                                            <td width="40%">[[item.id]]</td>
                                                                            <td width="60%"> <b>[[item.value]]</b></td>
                                                                        </tr>
                                                                    </table>
                                                                </template>
                                                            </vaadin-combo-box>                                                        
                                                        </template>
                                                    </vaadin-grid-column>
                                                    <vaadin-grid-column>
                                                        <template class="header">
                                                            <vaadin-grid-sorter path="clientNo">Cód Cliente</vaadin-grid-sorter>
                                                            <vaadin-grid-filter path="clientNo" />
                                                        </template>
                                                        <template>
                                                            <vaadin-text-field id="BPGridClientNo" name="BPClientNo" disabled value="{{item.clientNo}}">  </vaadin-text-field>
                                                        </template>
                                                    </vaadin-grid-column>
                                                    <vaadin-grid-column>
                                                        <template class="header">
                                                            <vaadin-grid-sorter path="clientName">Cliente</vaadin-grid-sorter>
                                                            <vaadin-grid-filter path="clientName" />
                                                        </template>
                                                        <template>
                                                            <vaadin-text-field id="BPGridClientName" name="BPClientName" disabled value="{{item.clientName}}">  </vaadin-text-field>
                                                        </template>
                                                    </vaadin-grid-column>
                                                    <vaadin-grid-column>
                                                        <template class="header">
                                                            <vaadin-grid-sorter path="totalSales">Total de Vendas</vaadin-grid-sorter>
                                                            <vaadin-grid-filter path="totalSales" />
                                                        </template>
                                                        <template>
                                                            <vaadin-text-field id="BPGridTotalSales" name="BPTotalSales" label="Total de Vendas" disabled="{{ !_permissions.update }}" value="{{item.totalSales}}">  </vaadin-text-field>
                                                        </template>
                                                    </vaadin-grid-column>
                                                    <vaadin-grid-column>
                                                        <template class="header">
                                                        </template>
                                                        <template>
                                                            <center>
                                                                <template is="dom-if" if="{{ _permissions.update }}">
                                                                    <paper-button class="btnRemove" raised on-tap="_deleteClientRequisition">Eliminar</paper-button>
                                                                </template>
                                                            </center>
                                                        </template>
                                                    </vaadin-grid-column>
                                                </vaadin-grid>
                                            </div>
                                        </div>
                                    </div>
                                </div>

                                <!-- Cafetarias / Refeitórios -->
                                <div>
                                    <div class="row break_clear">
                                        <div class="panel panel-default">
                                            <div class="panel-heading">Cafetarias / Refeitórios</div>
                                            <div class="panel-body">
                                                <!-- VAADIN GRID  Projetos de Faturação -->
                                                <vaadin-grid aria-label="BillingProjectsGrid" id="grid_ClientRequisitions" items="[[data.coffeeShops]]" class="grid" multi-sort="true" disabled="{{ !_permissions.update }}">
                                                    <vaadin-grid-column>
                                                        <template class="header">
                                                            <vaadin-grid-sorter path="type">Tipo</vaadin-grid-sorter>
                                                            <vaadin-grid-filter path="type" />
                                                        </template>
                                                        <template>
                                                            {{item.type}}
                                                        </template>
                                                    </vaadin-grid-column>
                                                    <vaadin-grid-column>
                                                        <template class="header">
                                                            <vaadin-grid-sorter path="code">Código</vaadin-grid-sorter>
                                                            <vaadin-grid-filter path="code" />
                                                        </template>
                                                        <template>
                                                            {{item.code}}
                                                        </template>
                                                    </vaadin-grid-column>
                                                    <vaadin-grid-column>
                                                        <template class="header">
                                                            <vaadin-grid-sorter path="description">Descrição</vaadin-grid-sorter>
                                                            <vaadin-grid-filter path="description" />
                                                        </template>
                                                        <template>
                                                            {{item.description}}
                                                        </template>
                                                    </vaadin-grid-column>
                                                    <vaadin-grid-column>
                                                        <template class="header">
                                                            <vaadin-grid-sorter path="projectNo">Projeto</vaadin-grid-sorter>
                                                            <vaadin-grid-filter path="projectNo" />
                                                        </template>
                                                        <template>
                                                            {{item.projectNo}}
                                                        </template>
                                                    </vaadin-grid-column>
                                                    <vaadin-grid-column>
                                                        <template class="header">
                                                            <vaadin-grid-sorter path="balance">Saldo</vaadin-grid-sorter>
                                                            <vaadin-grid-filter path="balance" />
                                                        </template>
                                                        <template>
                                                            {{item.balance}}
                                                        </template>
                                                    </vaadin-grid-column>
                                                    <vaadin-grid-column>
                                                        <template class="header">
                                                            <vaadin-grid-sorter path="totalCosts">Total Custos</vaadin-grid-sorter>
                                                            <vaadin-grid-filter path="totalCosts" />
                                                        </template>
                                                        <template>
                                                            {{item.totalCosts}}
                                                        </template>
                                                    </vaadin-grid-column>
                                                </vaadin-grid>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </iron-pages>
                            <div class="marginSpace">
                                <template is="dom-if" if="{{ _permissions.update }}">
                                    <template is="dom-if" if="{{ _isCreate }}">
                                        <paper-button raised id="btnCreate" class="btnSave" on-tap="_create"><i class="fa fa-plus" aria-hidden="true"></i>&nbsp; Criar</paper-button>
                                    </template>
                                    <template is="dom-if" if="{{ !_isCreate }}">
                                        <template is="dom-if" if="{{ _permissions.update }}">
                                            <paper-button raised id="btnUpdate" class="btnSave" on-tap="_update"><i class="fa fa-floppy-o" aria-hidden="true"></i>&nbsp; Guardar</paper-button>
                                            <paper-button raised id="btnArchive" class="btnSave" on-tap="_archive"><i class="fa fa-file-archive-o" aria-hidden="true"></i>&nbsp; Arquivar</paper-button>
                                        </template>
                                    </template>
                                    <template is="dom-if" if="{{ _permissions.delete }}">
                                        <paper-button raised id="btnDelete" class="btnRemove" on-tap="_delete"><i class="fa fa-trash-o" aria-hidden="true"></i>&nbsp; Eliminar</paper-button>
                                    </template>
                                </template>
                            </div>
                        </div>
                    </div>
                </iron-form>
            </div>
        </div>

        <paper-toast id="ToastError" style="background-color:red" duration="4000" horizontalAlign="right"></paper-toast>
        <paper-toast id="ToastMessage" duration="4000" horizontalAlign="right"></paper-toast>
    </template>
    <script>
        document.addEventListener('WebComponentsReady', function () {
            Polymer({
                is: 'x-productivityunitdetails',
                properties: {
                    _permissions: {
                        type: Array
                    },
                    selected: {
                        type: Number,
                        value: 0
                    },
                    _isCreate: {
                        type: Boolean,
                        value: true
                    },
                },
                _openToast: function (message) {
                    this.$.ToastMessage.text = message;
                    this.$.ToastMessage.open();
                },
                _openToastError: function (message) {
                    this.$.ToastError.text = message;
                    this.$.ToastError.open();
                },
                _validateData: function () {
                    if (true) {

                        return true;
                    }
                    this._openToastError("Verifique os dados inseridos.");
                    return false;
                },


                ready: function () {
                    this.$.grid_databound.body = JSON.stringify(@ViewBag.ProductivityUnityNo);
                    this.$.grid_databound.generateRequest();

                },

                //CRUDS
                _create: function () {
                    if (this._validateData()) {
                        this.$.grid_databound.url = "/Nutricao/UnidadesProdutivas/CreateProductivityUnit"
                        this.$.grid_databound.body = JSON.stringify(this.data);
                        this.$.grid_databound.generateRequest();
                    }
                },

                //RESPONSE PROCESSORES
                _responseProcessor: function (data) {
                    var self = this;

                    //Set create mode
                    if (!(this.data.productivityUnitNo == "" || this.data.productivityUnitNo == null)) {
                        this._isCreate = true;
                    }

                    if (data.detail.status === 200) {
                        if (data.detail.url.indexOf("Get") !== -1) {

                        } else if (data.detail.url.indexOf("Create") !== -1) {

                        } else if (data.detail.url.indexOf("Update") !== -1) {

                        }
                    } else {
                        this._openToastError("Ocorreu um erro desconhecido.")
                    }
                },

                //BILLING PROJECT
