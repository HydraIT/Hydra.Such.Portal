
@{
    ViewData["Title"] = "Detalhes Requisições Simplificadas";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<link rel="import" href="/lib/paper-tabs-master/paper-tab.html" />
<link rel="import" href="/lib/paper-tabs-master/paper-tabs.html" />
<link rel="import" href="/lib/iron-pages-master/iron-pages.html" />
<link rel="import" href="/lib/iron-menu-behavior/iron-menubar-behavior.html" />
<link rel="import" href="/lib/iron-form/iron-form.html" />
<link rel="import" href="/lib/paper-button/paper-button.html" />
<link rel="import" href="/lib/paper-input/paper-input.html" />
<link rel="import" href="/lib/paper-toast-master/paper-toast.html" />

<link rel="import" href="/lib/vaadin-valo-theme/vaadin-date-picker.html">
<link rel="import" href="~/lib/vaadin-date-picker/vaadin-date-picker.html" >

<link rel="import" href="/lib/vaadin-valo-theme/vaadin-checkbox.html">
<link rel="import" href="/lib/vaadin-text-field/vaadin-text-field.html">
<link rel="import" href="/lib/vaadin-grid/vaadin-grid.html">
<link rel="import" href="/lib/vaadin-grid/vaadin-grid-sorter.html">
<link rel="import" href="/lib/vaadin-grid/vaadin-grid-filter.html">
<link rel="import" href="/lib/vaadin-checkbox/vaadin-checkbox.html">
<link rel="import" href="/lib/vaadin-combo-box/vaadin-combo-box.html" />

@if (ViewBag.RequestNo == "" || ViewBag.RequestNo == "0")
{
    <h1 class="title">Criar Requisição Simplificada</h1>
}
else
{
    <h1 class="title">Requisição Simplificada @ViewBag.ContractNo</h1>
}

<x-simplifiedreqdetails _requestno="@ViewBag.RequestNo" _permissions='@Html.Raw(Json.Serialize(ViewBag.UPermissions))'></x-simplifiedreqdetails>

<dom-module id="x-simplifiedreqdetails">
    <template>
        <!-- STYLES -->
        <link rel="stylesheet" href="/lib/bootstrap/dist/css/bootstrap.css" />
        <link rel="stylesheet" href="/lib/font-awesome-4.7.0/css/font-awesome.min.css" />
        <link rel="stylesheet" href="/css/main.css" />
        <style>
            vaadin-combo-box {
                margin-top: 0px !important;
                width: 100% !important;
            }

            vaadin-date-picker {
                margin-top: 0px !important;
                width: 100% !important;
            }

            vaadin-text-field {
                margin-top: 0px !important;
                width: 100%;
            }

            .break_clear {
                margin-top: 15px;
            }
        </style>


        <!-- DROPDOWNS POPULATE -->
        <iron-ajax url="/PopulateDropdowns/GetFunctionalAreaCode" body="[{'UserId': '3'}]" last-response="{{CBFunctionalAreaCodes}}" auto method="post" handle-as="json" content-type="application/json"></iron-ajax>
        <iron-ajax url="/PopulateDropdowns/GetResponsabilityCenterCode" last-response="{{CBResponsabilityCenterCodes}}" auto method="post" handle-as="json" content-type="application/json"></iron-ajax>
        <iron-ajax url="/PopulateDropdowns/GetRegionCode" last-response="{{CBRegionCodes}}" auto method="post" handle-as="json" content-type="application/json"></iron-ajax>
        <iron-ajax url="/PopulateDropdowns/GetLocations" last-response="{{CBLocations}}" auto method="post" handle-as="json" content-type="application/json"></iron-ajax>
        <iron-ajax url="/PopulateDropdowns/GetProjectList" last-response="{{CBProjects}}" auto method="post" handle-as="json" content-type="application/json"></iron-ajax>
        <iron-ajax url="/PopulateDropdowns/GetMealTypes" last-response="{{CBMealType}}" auto method="post" handle-as="json" content-type="application/json"></iron-ajax>
        <iron-ajax url="/PopulateDropdowns/GetUnitStockeeping" last-response="{{CBProductos}}" auto method="post" handle-as="json" content-type="application/json"></iron-ajax>
        <iron-ajax url="/PopulateDropdowns/GetUnitOfMeasureByCode" last-response="{{CBMeasureUnits}}" auto method="post" handle-as="json" content-type="application/json"></iron-ajax>
        <iron-ajax url="/PopulateDropdowns/GetRequisitionsStatus" last-response="{{GetRequisitionsStatus}}" auto method="post" handle-as="json" content-type="application/json"></iron-ajax>
        <!-- PROJECT CRUDS -->
        <iron-ajax url="/Nutricao/RequisicoesSimplificadas/GetSimplifiedRequisitionData"
                   last-response="{{result}}"
                   method="post"
                   handle-as="json"
                   id="page_databound_ajax"
                   on-response="_responseProcessor"
                   content-type="application/json"></iron-ajax>

        <iron-ajax last-response="{{requisitions}}"
                   method="post"
                   handle-as="json"
                   id="register_requisitions_ajax"
                   on-response="_responseProcessorRegister"
                   content-type="application/json"></iron-ajax>



        <iron-ajax last-response="{{updateLines}}"
                   method="post"
                   handle-as="json"
                   on-response="_responseProcessorLines"
                   id="update_lines_requisition_ajax"
                   content-type="application/json"></iron-ajax>

        <iron-ajax url="/Nutricao/RequisicoesSimplificadas/DeleteSimplifiedRequisition"
                   last-response="{{deletedData}}"
                   method="post"
                   handle-as="json"
                   id="ajax_delete_project"
                   on-response="_responseDeleteProcessor"
                   content-type="application/json"></iron-ajax>

        <iron-ajax url="/Nutricao/RequisicoesSimplificadas/DeleteSimplifiedRequisitionLines"
                   last-response="{{deletedData}}"
                   method="post"
                   handle-as="json"
                   id="ajax_delete_lines_project"
                   on-response="_responseDeleteLinesProcessor"
                   content-type="application/json"></iron-ajax>


        <template is="dom-if" if="{{ !_isCreate }}">
            <div class="row">
                <div class="col-lg-12">
                    <template is="dom-if" if="{{ _permissions.create }}">
                        <paper-button raised id="btnProjectAdd" class="" on-tap="_createNew"><i class="fa fa-plus" aria-hidden="true"></i>&nbsp; Criar Novo</paper-button>
                    </template>
                </div>
            </div>
        </template>
        &nbsp;
        @* Botões de registar requisições aprovadas*@
        <div class="subMenu" id="ddmarginSpace3">
            <paper-button class="tablinks" on-tap="_registeRequisiApproval"><i class="fa fa-chevron-circle-down" aria-hidden="true"></i> Registar Linhas de Diário</paper-button>
            <paper-button class="tablinks" on-tap="_desaprovarRequisiApproval"><i class="fa fa-check-square" aria-hidden="true"></i> Desaprovar Requisições</paper-button>
            <paper-button class="tablinks" on-tap="_historyRequisiApproval"><i class="fa fa-check-square" aria-hidden="true"></i> Arquivar Requisições</paper-button>
        </div>
        &nbsp;
        <div class="row break_frm">
            <div class="col-md-12 col-lg-12">
                <iron-form id="formContract">
                    <div class="panel panel-default">
                        <div class="panel-heading">Cabeçalho</div>
                        <div class="panel-body">
                            <paper-tabs fit-container no-slide autoselect autoselect-delay="0" selected="{{selected}}">
                                <paper-tab>Geral</paper-tab>
                                <paper-tab>Detalhe</paper-tab>
                            </paper-tabs>
                            <iron-pages selected="{{selected}}">
                                <!--Geral-->
                                <div>
                                    <div class="row">
                                        <div class="col-lg-3">
                                            <vaadin-text-field value="{{result.requisitionNo}}" maxlength="20" disabled="{{ !_isCreate }}" id="lRequestNo" name="lRequestNo" label="Nº"> </vaadin-text-field>
                                        </div>
                                        <div class="col-lg-3">
                                            <vaadin-combo-box items="[[GetRequisitionsStatus]]" id="LStatus" selected="1" item-value-path="id" item-label-path="value" value="{{result.status}}" label="Estado" disabled>
                                                <template>
                                                    <b>[[item.value]]</b>
                                                </template>
                                            </vaadin-combo-box>
                                        </div>
                                        <div class="col-lg-3 alignPoga">
                                            <vaadin-checkbox checked="{{result.finished}}" disabled>Terminada</vaadin-checkbox>
                                        </div>
                                        <div class="col-lg-3">
                                            <vaadin-combo-box items="[[CBLocations]]" id="LocationCode" item-value-path="id" item-label-path="value" value="{{result.locationCode}}" label="Cód. Localização" disabled="{{ !_permissions.update }}" on-value-changed="_locationCodeChanged">
                                                <template>
                                                    <b>[[item.value]]</b>
                                                </template>
                                            </vaadin-combo-box>
                                        </div>
                                    </div>
                                    <div class="row break_clear">
                                        <div class="col-lg-3">
                                            <vaadin-combo-box items="[[CBProjects]]" id="ProjectNo" item-value-path="id" item-label-path="value" value="{{result.projectNo}}" label="Nº Projeto" disabled="{{ !_permissions.update }}" on-value-changed="_projectoNoChanged">
                                                <template>
                                                    <b>[[item.value]]</b>
                                                </template>
                                            </vaadin-combo-box>
                                        </div>
                                        <div class="col-lg-3">
                                            <vaadin-text-field value="{{result.employeeNo}}" maxlength="50" id="LEmployeeNo" name="LEmployeeNo" label="Nº Funcionário" disabled> </vaadin-text-field>
                                        </div>
                                        <div class="col-lg-3">
                                            <vaadin-text-field value="{{result.observations}}" maxlength="50" id="LObservations" name="LObservations" label="Observações" disabled="{{ !_permissions.create }}"> </vaadin-text-field>
                                        </div>
                                        <div class="col-lg-3">
                                            <vaadin-text-field value="{{result.totalCost}}" maxlength="20" id="LTotalCost" name="LTotalCost" label="Custo Total" disabled> </vaadin-text-field>
                                        </div>
                                    </div>
                                    <div class="row break_clear">
                                        <div class="col-lg-3">
                                            <vaadin-combo-box items="[[CBRegionCodes]]" id="RegionCode" item-value-path="id" item-label-path="value" value="{{result.regionCode}}" label="Cód. Região" disabled="{{ @ViewBag.LockFields }}" on-value-changed="_regionCodeChanged">
                                                <template>
                                                    <table width="180px">
                                                        <tr>
                                                            <td width="20%">[[item.id]]</td>
                                                            <td width="80%"> <b>[[item.value]]</b></td>
                                                        </tr>
                                                    </table>
                                                </template>
                                            </vaadin-combo-box>
                                        </div>
                                        <div class="col-lg-3">
                                            <vaadin-combo-box items="[[CBFunctionalAreaCodes]]" id="FunctionalAreaCode" item-value-path="id" item-label-path="value" value="{{result.functionalAreaCode}}" label="Cód. Área Funcional" disabled="{{ @ViewBag.LockFields }}" on-value-changed="_areaCodeChanged">
                                                <template>
                                                    <table width="200px">
                                                        <tr>
                                                            <td width="40%">[[item.id]]</td>
                                                            <td width="60%"> <b>[[item.value]]</b></td>
                                                        </tr>
                                                    </table>
                                                </template>
                                            </vaadin-combo-box>
                                        </div>
                                        <div class="col-lg-3">
                                            <vaadin-combo-box items="[[CBResponsabilityCenterCodes]]" id="ResponsabilityCenterCode" item-value-path="id" item-label-path="value" value="{{result.responsabilityCenterCode}}" label="Cód. Centro Responsabilidade" disabled="{{ @ViewBag.LockFields }}" on-value-changed="_responsabilityCenterCodeChanged">
                                                <template>
                                                    <table width="200px">
                                                        <tr>
                                                            <td width="40%">[[item.id]]</td>
                                                            <td width="60%"> <b>[[item.value]]</b></td>
                                                        </tr>
                                                    </table>
                                                </template>
                                            </vaadin-combo-box>
                                        </div>
                                        <div class="col-lg-3 ">
                                            <vaadin-date-picker label="Data Receção" id="LReceiptPreviewDate" name="LReceiptPreviewDate" value="{{result.receiptPreviewDate}}" disabled="{{ !_permissions.create }}"></vaadin-date-picker>
                                        </div>

                                    </div>
                                    <div class="row break_clear">
                                        <div class="col-lg-3 ">
                                            <vaadin-combo-box items="[[CBMealType]]" id="LMealType" item-value-path="id" item-label-path="value" on-value-changed="_mealTypeChanged" label="Tipo Refeição" disabled="{{ !_permissions.create }}">
                                                <template>
                                                    <b>[[item.id]]</b>
                                                    <b>[[item.value]]</b>
                                                </template>
                                            </vaadin-combo-box>
                                        </div>
                                    </div>
                                </div>

                                <!--Detalhe-->
                                <div>
                                    <div class="row break_clear">
                                        <div class="col-lg-3">
                                            <vaadin-text-field value="{{result.createResponsible}}" id="LCreateResponsible" name="LCreateResponsible" label="Responsável Criação" disabled> </vaadin-text-field>
                                        </div>
                                        <div class="col-lg-3">
                                            <vaadin-text-field value="{{result.requisitionDate}}" id="LRequisitionDate" name="LRequisitionDate" label="Data Requisição" disabled> </vaadin-text-field>
                                        </div>
                                        <div class="col-lg-3">
                                            <vaadin-text-field value="{{result.requisitionTime}}" id="LRequisitionTime" name="LRequisitionTime" label="Hora Requisição" disabled> </vaadin-text-field>
                                        </div>
                                        <div class="col-lg-3">
                                            <vaadin-text-field value="{{result.approvalResponsible}}" id="LApprovalResponsible" name="LApprovalResponsible" label="Responsável Aprovação" disabled> </vaadin-text-field>
                                        </div>
                                    </div>
                                    <br />
                                    <div class="row break_clear">
                                        <div class="col-lg-3">
                                            <vaadin-text-field value="{{result.approvalDate}}" id="LApprovalDate" name="LApprovalResponsible" label="Data Aprovação" disabled> </vaadin-text-field>
                                        </div>
                                    </div>
                                </div>
                            </iron-pages>

                            <div class="marginSpace" id="ddmarginSpace">
                                <template is="dom-if" if="{{ _isCreate }}">
                                    <paper-button raised id="btnCreate" class="btnSave" on-tap="_create"><i class="fa fa-plus" aria-hidden="true"></i>&nbsp; Criar</paper-button>
                                </template>
                                <template is="dom-if" if="{{ !_isCreate }}">
                                    <template is="dom-if" if="{{ _permissions.update }}">
                                        <paper-button raised id="btnUpdate" class="btnSave" on-tap="_update"><i class="fa fa-floppy-o" aria-hidden="true"></i>&nbsp; Guardar</paper-button>
                                    </template>
                                    <template is="dom-if" if="{{ _permissions.delete }}">
                                        <paper-button raised id="btnCancel" class="btnRemove" on-tap="_delete"><i class="fa fa-trash-o" aria-hidden="true"></i>&nbsp; Eliminar</paper-button>

                                    </template>
                                </template>
                            </div>
                            <div class="marginSpace" id="ddmarginSpace2">
                                <template is="dom-if" if="{{ _isCreate }}">
                                    <paper-button raised id="btnUpdate" class="btnSave" on-tap="_update"><i class="fa fa-floppy-o" aria-hidden="true"></i>&nbsp; Guardar</paper-button>
                                </template>
                            </div>

                        </div>
                    </div>
                </iron-form>
            </div>
        </div>

        <hr />

        <!-- ############################ ############################ -->
        <!-- ####### Contract Lines ##### ####### Contract Lines ##### -->
        <!-- ############################ ############################-->

        <iron-ajax url="/Nutricao/RequisicoesSimplificadas/GetSimplifiedRequisitionLines"
                   last-response="{{dataLines}}"
                   method="post"
                   handle-as="json"
                   id="contractLines_databound_ajax"
                   content-type="application/json"></iron-ajax>

        <div class="row break_frm" style="display: {{ _showLines }};">
            <div class="col-md-12 col-lg-12">
                <div class="panel panel-default">
                    <div class="panel-heading">Linhas</div>
                    <div class="panel-body">
                        <div id="formLinesReq">
                            <div class="row" style="display: {{ _showLinesForm }};">
                                <div class="col-lg-2">
                                    <vaadin-combo-box items="[[CBLocations]]" id="_CLLocationCode" name="_LLLocation" item-value-path="id" item-label-path="value" value="{{_LLLocation}}" label="Localização" disabled="{{ !_permissions.update }}">
                                        <template>
                                            <b>[[item.value]]</b>
                                        </template>
                                    </vaadin-combo-box>
                                </div>
                                <div class="col-lg-2">
                                    <vaadin-combo-box items="[[CBProductos]]" id="_CLCode" name="_LLProduct" item-value-path="id" item-label-path="id" value="{{_LLProduct}}" label="Nº Produto" disabled="{{ !_permissions.update }}" on-value-changed="_productChanged">
                                        <template>
                                            <b>[[item.id]]</b>
                                            <b>[[item.value]]</b>
                                        </template>
                                    </vaadin-combo-box>
                                </div>

                                <div class="col-lg-2">
                                    <vaadin-text-field value="{{_LLDescription}}" id="_CLDescription" name="_LLDescription" label="Descrição" disabled="{{ !_permissions.update }}"> </vaadin-text-field>
                                </div>
                                <div class="col-lg-2">
                                    <vaadin-text-field value="{{_LLUnitCost}}" type="number" id="_CLUnitCost" name="_LLDescription" label="Preço Unico" disabled>  €</vaadin-text-field>
                                </div>
                                <div class="col-lg-2">
                                    <vaadin-combo-box items="[[CBMeasureUnits]]" id="_CLMeasureUnitNo" name="_LLMeasureUnit" item-value-path="id" item-label-path="value" value="{{_LLMeasureUnit}}" label="Cód. Unidade Medida" disabled="{{ !_permissions.update }}">
                                        <template>
                                            <b>[[item.value]]</b>
                                        </template>
                                    </vaadin-combo-box>
                                </div>

                                <div class="col-lg-2">
                                    <vaadin-text-field value="{{_LLQuantityToRequire}}" type="number" id="_CLQuantityToRequire" name="_LLQuantityToRequire" label="Quantidade a requerer" disabled="{{ !_permissions.update }}" on-value-changed="_readQuantity"> </vaadin-text-field>
                                </div>
                               
                            </div>

                            <div class="row" style="display: {{ _showLinesForm }};">
                                <div class="col-lg-2">
                                    <vaadin-text-field value="{{_LLTotalCost}} " id="_CLTotalCost" name="_LLTotalCost" label="Custo Total" disabled> </vaadin-text-field>
                                </div>
                                <div class="col-lg-2">
                                    <vaadin-combo-box items="[[CBProjects]]" id="_CLProjectNo" item-value-path="id" item-label-path="value" value="{{result.projectNo}}" label="Nº Projeto" disabled="{{ _lockFields }}">
                                        <template>
                                            <b>[[item.value]]</b>
                                        </template>
                                    </vaadin-combo-box>
                                </div>

                                <div class="col-lg-2">
                                    <vaadin-combo-box items="[[CBMealType]]" id="_CLMealType" item-value-path="id" item-label-path="value" value="{{result.mealType}}" label="Tipo Refeição" disabled="{{ _lockFields }}">
                                        <template>
                                            <b>[[item.value]]</b>
                                        </template>
                                    </vaadin-combo-box>
                                </div>


                                <div class="col-lg-2">
                                    <vaadin-date-picker label="Data Registo" id="_CLRegistrationDate" name="_CLRegistrationDate" value="{{_LLRegistrationDate}}" disabled="{{ !_permissions.update }}"></vaadin-date-picker>
                                </div>

                                <div class="col-lg-2">
                                    <vaadin-combo-box items="[[CBRegionCodes]]" id="_CLRegionCode" name="_CLRegion" item-value-path="id" item-label-path="value" value="{{result.regionCode}}" label="Cód. Região" disabled="{{ _lockFields }}">
                                        <template>
                                            <table width="180px">
                                                <tr>
                                                    <td width="20%">[[item.id]]</td>
                                                    <td width="80%"> <b>[[item.value]]</b></td>
                                                </tr>
                                            </table>
                                        </template>
                                    </vaadin-combo-box>
                                </div>

                                <div class="col-lg-2">
                                    <vaadin-combo-box items="[[CBFunctionalAreaCodes]]" id="_CLFunctionalArea" name="_CLFunctionalArea" item-value-path="id" item-label-path="value" value="{{result.functionalAreaCode}}" label="Cód. Área Funcional" disabled="{{ _lockFields }}">
                                        <template>
                                            <table width="200px">
                                                <tr>
                                                    <td width="40%">[[item.id]]</td>
                                                    <td width="60%"> <b>[[item.value]]</b></td>
                                                </tr>
                                            </table>
                                        </template>
                                    </vaadin-combo-box>
                                </div>
                                <div class="col-lg-3">
                                    <vaadin-combo-box items="[[CBResponsabilityCenterCodes]]" id="_CLCenterCodeResponsability" item-value-path="id" item-label-path="value" value="{{result.responsabilityCenterCode}}" label="Cód. Centro Responsabilidade" disabled="{{_lockFields }}">
                                        <template>
                                            <table width="200px">
                                                <tr>
                                                    <td width="40%">[[item.id]]</td>
                                                    <td width="60%"> <b>[[item.value]]</b></td>
                                                </tr>
                                            </table>
                                        </template>
                                    </vaadin-combo-box>
                                </div>
                            </div>
                            <div class="marginSpace" id="ddmarginSpace">
                                <paper-button raised id="btnCreateLine" class="btnSave" on-tap="_createContractLine"><i class="fa fa-plus" aria-hidden="true"></i>&nbsp; Criar</paper-button>

                            </div>
                            </div>
                            <div class="row">
                                <vaadin-grid aria-label="RequisitionLines" id="grid_requisitionlines" name="grid_requisitionlines" items="[[dataLines]]" class="grid" multi-sort="true" active-item="{{_SelectedLine}}">
                                    <vaadin-grid-column width="100px">
                                        <template class="header">
                                            <vaadin-grid-sorter path="requisitionNo">Nº</vaadin-grid-sorter>
                                            <vaadin-grid-filter path="requisitionNo" />
                                        </template>
                                        <template>
                                            {{item.requisitionNo}}
                                        </template>
                                    </vaadin-grid-column>
                                    <vaadin-grid-column width="125px">
                                        <template class="header">
                                            <vaadin-grid-sorter path="code">Nº Produto</vaadin-grid-sorter>
                                            <vaadin-grid-filter path="code" />
                                        </template>
                                        <template>
                                            <template is="dom-if" if="{{ item.selected }}">
                                                <vaadin-combo-box items="[[CBProductos]]" id="_LLGridProductNo" name="_LLGridProductNo" item-value-path="id" item-label-path="value" value="{{item.code}}" on-change="_onChangeProduct" >
                                                    <template>
                                                        <paper-icon-item>
                                                            <paper-item-body two-line>
                                                                <div>[[item.value]]</div>
                                                            </paper-item-body>
                                                        </paper-icon-item>
                                                    </template>
                                                </vaadin-combo-box>
                                            </template>
                                            <template is="dom-if" if="{{ !item.selected }}">
                                                {{item.code}}
                                            </template>
                                        </template>
                                    </vaadin-grid-column>
                                    <vaadin-grid-column width="200px">
                                        <template class="header">
                                            <vaadin-grid-sorter path="locationCode">Localização</vaadin-grid-sorter>
                                            <vaadin-grid-filter path="locationCode" />
                                        </template>
                                        <template>
                                            <template is="dom-if" if="{{ item.selected }}">
                                                <vaadin-combo-box items="[[CBLocations]]" id="_CLGridCode" name="_LLGridCode" item-value-path="id" item-label-path="value" value="{{item.locationCode}}" >
                                                    <template>
                                                        <b>[[item.value]]</b>
                                                    </template>
                                                </vaadin-combo-box>
                                            </template>
                                            <template is="dom-if" if="{{ !item.selected }}">
                                                {{item.locationCode}}
                                            </template>
                                        </template>
                                    </vaadin-grid-column>
                                    <vaadin-grid-column width="200px">
                                        <template class="header">
                                            <vaadin-grid-sorter path="description">Descrição</vaadin-grid-sorter>
                                            <vaadin-grid-filter path="description" />
                                        </template>
                                        <template>
                                            <template is="dom-if" if="{{ item.selected }}">
                                                <vaadin-text-field value="{{item.description}}" id="_LLGridDescription" name="_LLGridDescription" disabled> </vaadin-text-field>
                                            </template>
                                            <template is="dom-if" if="{{ !item.selected }}">
                                                {{item.description}}
                                            </template>
                                        </template>
                                    </vaadin-grid-column>
                                    <vaadin-grid-column width="170px">
                                        <template class="header">
                                            <vaadin-grid-sorter path="measureUnitNo">Cód. Uni. Medida</vaadin-grid-sorter>
                                            <vaadin-grid-filter path="measureUnitNo" />
                                        </template>
                                        <template>
                                            <template is="dom-if" if="{{ item.selected }}">
