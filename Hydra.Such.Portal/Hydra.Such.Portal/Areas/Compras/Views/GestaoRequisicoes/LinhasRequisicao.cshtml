    <script>
        document.addEventListener('WebComponentsReady', function () {
            Polymer({
                is: 'x-requestlines',
                properties: {
                    selected: {
                        type: Number,
                        value: 0
                    }
                },
                _create() {
                        this.data.unshift({
                            requestNo: this.$.requestNoId.value,
                            type: this.$.typeId.value,
                            code: this.$.codeId.value,
                            description: this.$.descriptionId.value,
                            unitMeasureCode: this.$.unitMeasureCodeId.value,
                            localCode: this.$.localCodeId.value,
                            localMarket: this.$.localMarketId.value,
                            quantityToRequire: this.$.quantityToRequireId.value,
                            quantityRequired: this.$.quantityRequiredId.value,
                            quantityToProvide: this.$.quantityToProvideId.value,
                            quantityAvailable: this.$.quantityAvailableId.value,
                            quantityReceivable: this.$.quantityReceivableId.value,
                            quantityReceived: this.$.quantityReceivedId.value,
                            quantityPending: this.$.quantityPendingId.value,
                            unitCost: this.$.unitCostId.value,
                            expectedReceivingDate: this.$.expectedReceivingDateId.value,
                            billable: this.$.billableId.value,
                            projectNo: this.$.projectNoId.value,
                            regionCode: this.$.regionCodeId.value,
                            functionalAreaCode: this.$.functionalAreaCodeId.value,
                            centerResponsibilityCode: this.$.centerResponsibilityCodeId.value,
                            functionalNo: this.$.functionalNoId.value,
                            vehicle: this.$.vehicleId.value,
                            qtyByUnitOfMeasure: this.$.qtyByUnitOfMeasureId.value,
                            unitCostsould: this.$.unitCostsouldId.value,
                            budgetValue: this.$.budgetValueId.value,
                            maintenanceOrderLineNo: this.$.maintenanceOrderLineNoId.value,
                            createMarketSearch: this.$.createMarketSearchId.value,
                            submitPrePurchase: this.$.submitPrePurchaseId.value,
                            sendPrePurchase: this.$.sendPrePurchaseId.value,
                            localMarketDate: this.$.localMarketDateId.value,
                            localMarketUser: this.$.localMarketUserId.value,
                            sendForPurchase: this.$.sendForPurchaseId.value,
                            sendForPurchaseDate: this.$.sendForPurchaseDateId.value,
                            purchaseValidated: this.$.purchaseValidatedId.value,
                            purchaseRefused: this.$.purchaseRefusedId.value,
                            reasonToRejectionLocalMarket: this.$.reasonToRejectionLocalMarketId.value, 
                            rejectionLocalMarketDate: this.$.rejectionLocalMarketDateId.value,
                            purchaseId: this.$.purchaseIdId.value,
                            supplierNo:this.supplierNoId.value,
                            openOrderNo: this.$.openOrderNoId.value,
                            openOrderLineNo: this.$.openOrderLineNoId.value,
                            queryCreatedMarketNo: this.$.queryCreatedMarketNoId.value,
                            createdOrderNo: this.$.createdOrderNoId.value,
                            supplierProductCode: this.$.supplierProductCodeId.value,
                            unitNutritionProduction: this.$.unitNutritionProductionId.value,
                            marketLocalRegion: this.$.marketLocalRegionId.value,
                            customerNo: this.$.customerNoId.value,
                            approvers: this.$.approversId.value,
                                });
                        // Save to DB
                        this.$.ajax_updatedata.body = JSON.stringify(this.data);
                        this.$.ajax_updatedata.generateRequest();
                        this.$.requestGrid.clearCache();
                },
                _delete: function (e) {
                    var index = this.data.indexOf(e.model.item);
                    this.data.splice(index, 1);
                    // Save to DB
                    this.$.ajax_updatedata.body = JSON.stringify(this.data);
                    this.$.ajax_updatedata.generateRequest();
                    this.$.requestGrid.clearCache();
                }
            });
        });
    </script>

