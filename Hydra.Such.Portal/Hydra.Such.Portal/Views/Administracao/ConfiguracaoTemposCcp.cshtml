    <script>
        document.addEventListener('WebComponentsReady', function () {
            Polymer({
                is: 'x-ccptimeconfig',
                properties: {
                    _permissions: {
                        type: Array,
                        value: @Html.Raw(Json.Serialize(ViewBag.UPermissions))
                    },
                    _tipo: {
                        type: Number,
                        value: 0
                    },
                    _estado0: {
                        type: Number,
