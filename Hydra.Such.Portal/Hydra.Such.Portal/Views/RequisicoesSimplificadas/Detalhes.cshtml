    <script>
        document.addEventListener('WebComponentsReady', function () {
            Polymer({
                is: 'x-simplifiedreqdetails',
                properties: {
                    _requestno: {
                        type: String,
                        notify: true,
                        value: ""
                    },
                    _permissions: {
                        type: Array
                    },
                    _isCreate: {
                        type: Boolean,
                        value: true
                    },
                    _showLinesForm: {
                        type: String,
                        value: ""
                    },
                    _showLines: {
                        type: String,
                        value: "none"
                    },
                    _lockFields: {
                        type: Boolean,
                        value: true
                    },
                    _SelectedLine: {
                        observer: '_lineSelected'
                    },
                     activeItem: {
                        observer: '_activeItemChanged'
                    },
                    selected: {
                        type: Number,
                        value: 0
                    },
                },


                ready: function () {

                    //GET PROJECT DATA
                    if (this._requestno != "") {
                        this._isCreate = false;
                        this._showLines = '';
                        this.$.page_databound_ajax.body = JSON.stringify({ "RequisitionNo": this._requestno });
                        this.$.contractLines_databound_ajax.body = JSON.stringify({ "RequisitionNo": this._requestno });
                    }

                    if ("@ViewBag.Approval" == "registar") {
                        this._showLinesForm = 'none';
                        this.$.ddmarginSpaceCabe.style.display = 'none';
                    }
                    else if ("@ViewBag.Approval" == "historico") {
                        this._showLinesForm = 'none';
                        this.$.ddsubMenu.style.display = 'none';
                        this.$.ddmarginSpaceCabe.style.display = 'none';
                    }
                    else {
                        if (this._isCreate == true) {

                            this._showLines = 'none';
                        }
                        this.$.ddsubMenu.style.display = 'none';

                    }

                    this.$.page_databound_ajax.generateRequest();
                    this.$.contractLines_databound_ajax.url = "/RequisicoesSimplificadas/GetSimplifiedRequisitionLinesData";
                    this.$.contractLines_databound_ajax.generateRequest();

                },


                _createNew: function () {
                    '<%Session["aprovadoSession"] = " new"%>';
                    window.location.href = "/RequisicoesSimplificadas/Detalhes";
                },

                //CRUDS
                _finishRequisition: function () {
                    var erro = false;
                    if (this.dataLines != undefined) {
                        for (i = 0; i < this.dataLines.length; i++) {
                            if (this.dataLines[i].quantityToRequire == 0) {
                                erro = true;
                                break;
                            }
                        }
                    }
                    if (erro == false) {
                        this.$.register_requisitions_ajax.url = "/RequisicoesSimplificadas/FinishSimplifiedRequisition";
                        this.$.register_requisitions_ajax.body = JSON.stringify(this.dataLines);
                        this.$.register_requisitions_ajax.generateRequest();
                    }
                    else {
                        this._openToastError("Preencha quantidade em uma ou mais linhas!");
                    }
                },
                _confirmTemplate: function () {
                    if (this.$.projecModelsTable.selectedItems.length != 0) {

                        var line;
                        if (this.$.projecModelsTable.selectedItems[0].requisitionNo == undefined)
                            line = { RequisitionNo: this.$.projecModelsTable.selectedItems[0].requisitionTemplateId };
                        else
                            line = { RequisitionNo: this.$.projecModelsTable.selectedItems[0].requisitionNo };

                        this._openToast("Modelo Utilizado com sucesso");
                        this.$.projecModelsTable.selectedItems = [];
                        this.$.page_databound_ajax2.body = JSON.stringify(line);
                        this.$.page_databound_ajax2.generateRequest();
                        this.$.dialog.close();
                    }
                    else {
                        this._openToastError("Selecione um modelo!");

                    }


                },
                _createContractLine: function () {

                    if (this._validateData("formcontractLines")) {

                        var addline = {
                            requisitionNo: this.$.lRequestNo.value,
                            locationCode: this.$._CLLocationCode.value,
                            code: this.$._CLCode.value,
                            description: this.$._CLDescription.value,
                            measureUnitNo: this.$._CLMeasureUnitNo.value,
                            quantityToRequire: this.$._CLQuantityToRequire.value,
                            totalCost: this.$._CLTotalCost.value,
                            unitCost: this.$._CLUnitCost.value,
                            requisitionDate: this.$._CLRegistrationDate.value,
                            projectNo: this.$._CLProjectNo.value,
                            mealType: this.$._CLMealType.value,
                            regionCode: this.$._CLRegionCode.value,
                            functionAreaCode: this.$._CLFunctionalArea.value,
                            responsabilityCenterCode: this.$._CLCenterCodeResponsability.value,
                            quantityApproved: 0,
                            quantityReceipt: 0,
                            quantityToApprove: 0
                        };

                        if (this.dataLines == null) {
                            this.dataLines = addline;
                        }
                        else {
                            this.dataLines.push(addline);
                        }
                        if (this.$.LTotalCost.value != null)
                            this.$.LTotalCost.value = this.$.LTotalCost.value + addline.totalCost;
                        else
                            this.$.LTotalCost.value = addline.totalCost;


                        this.$.update_lines_requisition_ajax.url = "/RequisicoesSimplificadas/CreateLinesSimplifiedRequisition";
                        this.$.update_lines_requisition_ajax.body = JSON.stringify(addline);
                        this.$.update_lines_requisition_ajax.generateRequest();
                        this.$.grid_requisitionlines.clearCache();

                    }
                },
                _create: function () {
                    if (this._validateData("formContract")) {
                        this.result.requisitionNo = this._requestno;
                        this.$.page_validator_ajax.body = JSON.stringify(this.result);
                        this.$.page_validator_ajax.generateRequest();
                    }
                },
                _update: function () {
                    this.$.update_lines_requisition_ajax.url = "/RequisicoesSimplificadas/UpdateSimplifiedRequisition";
                    this.$.update_lines_requisition_ajax.body = JSON.stringify(this.result);
                    this.$.update_lines_requisition_ajax.generateRequest();

                },
                _desaprovarRequisiApproval: function () {
                    this.$.register_requisitions_ajax.url = "/RequisicoesSimplificadas/DisapproveSimplifiedRequisition";
                    this.$.register_requisitions_ajax.body = JSON.stringify(this.dataLines);
                    this.$.register_requisitions_ajax.generateRequest();
                },
                _registeRequisiApproval: function () {
                    this.$.register_requisitions_ajax.url = "/RequisicoesSimplificadas/RegisterRequisition";
                    this.$.register_requisitions_ajax.body = JSON.stringify(this.dataLines);
                    this.$.register_requisitions_ajax.generateRequest();
                },
                _updateResquesitionLines: function () {
                    this.$.update_lines_requisition_ajax.url = "/RequisicoesSimplificadas/UpdateSimplifiedRequisitionLines";
                    this.$.update_lines_requisition_ajax.body = JSON.stringify(this.dataLines);
                    this.$.update_lines_requisition_ajax.generateRequest();
                },
                _historyRequisiApproval: function () {
                    this.$.register_requisitions_ajax.url = "/RequisicoesSimplificadas/HistorySimplifiedRequisition";
                    this.$.register_requisitions_ajax.body = JSON.stringify(this.dataLines);
                    this.$.register_requisitions_ajax.generateRequest();
                },
                _delete: function () {
                    var data = this.result;
                    var ajax_request = this.$.ajax_delete_project;
                    bootbox.confirm({
                        message: "Tem a certeza que pretende remover a Requisição Simplificada ?",
                        buttons: {
                            confirm: {
                                label: 'Sim',
                                className: 'btn-success'
                            },
                            cancel: {
                                label: 'Não',
                                className: 'btn-danger'
                            }
                        },
                        callback: function (result) {
                            if (result) {
                                ajax_request.body = JSON.stringify(data);
                                ajax_request.generateRequest();
                            }
                        }
                    });
                },

                _deleteRequisitionsLine: function (data) {
                    var ajax_request = this.$.ajax_delete_lines_project;
                    var dataline = data.model.item;
                    var index = this.dataLines.indexOf(dataline);
                    this.dataLines.splice(index, 1);

                    bootbox.confirm({
                        message: "Tem a certeza que pretende remover a  Linha Requisição Simplificada ?",
                        buttons: {
                            confirm: {
                                label: 'Sim',
                                className: 'btn-success'
                            },
                            cancel: {
                                label: 'Não',
                                className: 'btn-danger'
                            }
                        },
                        callback: function (result) {
                            if (result) {
                                ajax_request.body = JSON.stringify(dataline);
                                ajax_request.generateRequest();

                            }
                        }
                    });

                },
                _copyModel: function (data) {
                    this.$.get_models_ajax.url = "/ModelosReqSimplificada/GetAllReqTemplates";
                    //this.$.get_models_ajax.body = JSON.stringify(this.dataLines);
                    this.$.get_models_ajax.generateRequest();
                },

                _copyHistory: function (data) {
                    this.$.get_models_ajax.url = "/RequisicoesSimplificadas/SimplifiedRequisitionsPage";
                    this.$.get_models_ajax.body = JSON.stringify("historico");
                    this.$.get_models_ajax.generateRequest();

                },



                //Responses Processors
                _responseReqValidatorProcessor: function (data) {
                    var self = this;
                    debugger;
                    if (data.detail.status === 200) {
                        if (this.validateResponse == "") {
                            this._executeRequest("/RequisicoesSimplificadas/CreateSimplifiedRequisition", this.result)
                        } else {
                            this._openToastError(this.validateResponse);
                        }
                    }
                },
                _responseGetModels: function (data) {
                    this.$.dialog.open();
                },
                _responseContractLines: function () {

                    var costTotal = 0;
                    if (this.dataLines != undefined) {
                        for (var i = 0; i < this.dataLines.length; i++) {
                            costTotal = costTotal + this.dataLines[i].totalCost;
                        }
                    }
                    this.$.LTotalCost.value = costTotal;
                },
                _responseProcessor: function (data) {
                    var self = this;
                    if (data.detail.status === 200) {
                        this._requestno = this.result.requisitionNo;
                        if (data.detail.url.indexOf("Update") !== -1 || data.detail.url.indexOf("Create") !== -1) {
                            if (this.result.eReasonCode == 100) {
                                if (this.dataLines != undefined) {
                                    this.$.updatemultiLine_ajax.url = "/RequisicoesSimplificadas/CreateMultiLinesSimplifiedRequisition";
                                    this.$.updatemultiLine_ajax.body = JSON.stringify(this.dataLines);
                                    this.$.updatemultiLine_ajax.generateRequest();
                                    this.$.grid_requisitionlines.clearCache();
                                }
                                this._openToast(this.result.eMessage);
                                this._isCreate = false;
                                this._showLines = '';
                            } else {
                                this._openToastError(this.result.eMessage);
                                this._isCreate = true;
                                this._showLines = 'none';
                                this.ready();
                            }
                        }

                    } else {
                        this._openToastError("Ocorreu um erro.")
                    }
                },
                _responseProcessor2: function (data) {
                   this.$.contractLines_databound_ajax.url = "/RequisicoesSimplificadas/GetSimplifiedRequisitionLinesModels";
                   this.$.contractLines_databound_ajax.body = JSON.stringify({ "requestNo": this.result.requisitionNo, "requestNoNew": this._requestno});
                   this.result.requisitionNo = this._requestno;
                   this.$.contractLines_databound_ajax.generateRequest();
                   this.$.grid_requisitionlines.clearCache();
                },
                _responseProcessorLines: function (dataUpdate) {

                    if (dataUpdate.detail.status === 200) {
                        if (this.updateLines.eReasonCode == 100) {
                            if (this.updateLines.lineNo != undefined) {
                                this.$._CLLocationCode.value = "";
                                this.$._CLCode.value = "";
                                this.$._CLDescription.value = "";
                                this.$._CLMeasureUnitNo.value = "";
                                this.$._CLQuantityToRequire.value = "";
                                this.$._CLUnitCost.value = "";
                                this.dataLines[this.dataLines.length - 1].lineNo = this.updateLines.lineNo;
                            }
                            this._openToast(this.updateLines.eMessage);
                            this.$.grid_requisitionlines.clearCache();

                        } else if (this.updateLines.eReasonCode == 101) {
                            this._openToastError(this.updateLines.eMessage);
                        }
                        else if (this.updateLines != null) {
                            this._openToast("Requisição actualizada com sucesso");

                        }
                        else {
                            this._openToastError("Erro na actualização da requisição");
                        }


                    } else {
                        this._openToastError("Ocorreu um erro.")
                    }
                },
                _responseProcessorRegister: function (data) {
                    if (data.detail.status === 200) {
                        if (this.requisitions.eReasonCode == 100) {
                            this._openToast(this.requisitions.eMessage);
                            if ("@ViewBag.Approval" == "registar")
                                setTimeout(
                                    function () {
                                        window.location.href = "/RequisicoesSimplificadas/Index/?option=" + 1;
                                    }, 2500);
                            else
                                setTimeout(
                                    function () {
                                        window.location.href = "/RequisicoesSimplificadas/Index";
                                    }, 2500);

                        }
                        else {
                            this._openToastError(this.requisitions.eMessage);
                         }

                    } else {
                        this._openToastError("Ocorreu um erro.")
                    }
                },
                _responseDeleteProcessor: function (data) {
                    var self = this;
                    if (data.detail.status === 200) {
                        this._openToast(this.deletedData.eMessage);
                        if (this.deletedData.eReasonCode == 100) {

                            setTimeout(
                                function () {
                                    window.location.href = "/RequisicoesSimplificadas";
                                }, 2500);
                        } else if (this.deletedData.eReasonCode == 101) {
                            this._openToastError(this.deletedData.eMessage);
                        }
                    }
                },
                _responseDeleteLinesProcessor: function (data) {
                    var self = this;
                    if (data.detail.status === 200) {
                        this._openToast(this.deletedData.eMessage);
                        if (this.deletedData.eReasonCode == 100) {
                            this.$.LTotalCost.value = this.$.LTotalCost.value - this.deletedData.totalCost;
                            this.$.grid_requisitionlines.clearCache();
                            this._openToast(this.deletedData.eMessage);
                        } else if (this.deletedData.eReasonCode == 101) {
                            this._openToastError(this.deletedData.eMessage);
                        }
                    }
                },



                //Helpers
                _openToast: function (message) {
                    this.$.ToastMessageSuccess.text = message;
                    this.$.ToastMessageSuccess.open();
                },
                _openToastError: function (message) {
                    this.$.ToastMessageError.text = message;
                    this.$.ToastMessageError.open();
                },

                _validateData: function (container) {

                    var polymerDom = Polymer.dom(this.$[container]);
                    var errors = 0;
                    var formSelector = "vaadin-text-field, vaadin-date-picker, vaadin-combo-box";
                    var formFields = polymerDom.querySelectorAll(formSelector);

                    formFields.forEach(function (element) {
                        if (!element.validate()) errors++;
                    });

                    if (errors > 0) {
                        this._openToastError("Verifique os dados inseridos.");
                        return false;
                    }

                    return true;
                },
                _executeRequest: function (url, data) {
                    this.$.page_databound_ajax.url = url;
                    this.$.page_databound_ajax.body = JSON.stringify(data);
                    this.$.page_databound_ajax.generateRequest();

                },
                _onChangeProduct: function (data) {
                    if (data.model != null && this.CBProductos != undefined) {
                        var sel = this.CBProductos.find(x => x.id == data.model.item.code);
                        if (sel != undefined) {
                            this.dataLines.find(x => x == this._SelectedLine).description = sel.value;
                            this.dataLines.find(x => x == this._SelectedLine).measureUnitNo = sel.extra2;
                            this.$.grid_requisitionlines.clearCache();
                        }
                    }
                },

                _activeItemChanged: function (item) {
                    this.$.projecModelsTable.selectedItems = item ? [item] : [];
                },
                _lineSelected: function (item) {
                    if (item != null) {
                        var oSel = this.dataLines.find(x => x.selected);
                        if (oSel != undefined) {
                            oSel.selected = false;
                        }

                        var sel = this.dataLines.find(x => x == item);
                        if (sel != undefined) {

                            this.dataLines.find(x => x == item).selected = true;
                            this.$.grid_requisitionlines.clearCache();
                        }
                    }
                },
                _productChanged: function (param) {
                    if (param != undefined && this.CBProductos != undefined) {
                        var sel = this.CBProductos.find(x => x.id == param.detail.value);
                        if (sel != undefined) {
                            this.$._CLDescription.value = sel.value;
                            this.$._CLUnitCost.value = parseInt(sel.extra);
                            this.$._CLMeasureUnitNo.value =  sel.extra2;
                        }
                    }
                },
                _readQuantity: function (param) {
                    if (param != "" && this.$._CLUnitCost != undefined) {
                        if (this.$._CLUnitCost != undefined) {
                            this.$._CLTotalCost.value = param.detail.value * this.$._CLUnitCost.value;
                        }
                        else
                            this.$._CLTotalCost.value = 0;
                    }
                },
                _mealTypeChanged: function (e) {
                    if (this.dataLines != null && e.detail.value != null && e.detail.value != "") {
                        var count = this.dataLines.length;
                        for (var i = 0; i < count; i++) {
                            this.dataLines[i].mealType = e.detail.value;
                            //description meal

                            var meals = $.grep(this.CBMealType, function (x) { return x.id == e.detail.value; });
                            if (meals != null && meals.length > 0) {
                                var meal = meals[0];
                                this.dataLines[i].descriptionMeal = meal.value;
                            }
                        }
                        this.$.grid_requisitionlines.clearCache();

                    }
                },

                _regionCodeChanged: function () {
                    if (this.dataLines != null) {
                        var count = this.dataLines.length;
                        for (var i = 0; i < count; i++) {
                            this.dataLines[i].regionCode = this.$.RegionCode.value;
                        }
                        this.$.grid_requisitionlines.clearCache();

                    }
                },
                _projectoNoChanged: function () {
                    if (this.dataLines != null) {
                        var count = this.dataLines.length;
                        for (var i = 0; i < count; i++) {
                            this.dataLines[i].projectNo = this.$.ProjectNo.value;
                        }
                        this.$.grid_requisitionlines.clearCache();

                    }
                },
                _locationCodeChanged: function () {
                    if (this.dataLines != null) {
                        var count = this.dataLines.length;
                        for (var i = 0; i < count; i++) {
                            this.dataLines[i].locationCode = this.$.LocationCode.value;
                        }
                        this.$.grid_requisitionlines.clearCache();

                    }
                },
                _areaCodeChanged: function () {
                    if (this.dataLines != null) {
                        var count = this.dataLines.length;
                        for (var i = 0; i < count; i++) {
                            this.dataLines[i].functionAreaCode = this.$.FunctionalAreaCode.value;

                        }
                        this.$.grid_requisitionlines.clearCache();

                    }
                },
               _productNoChanged: function () {
                    if (this.dataLines != null) {
                        var count = this.dataLines.length;
                        for (var i = 0; i < count; i++) {
                            this.dataLines[i]._productNoChanged = this.$._productNoChanged.value;

                        }
                        this.$.grid_requisitionlines.clearCache();

                    }
                },
               _getRegionDescription: function (id) {
                   if (id != undefined && this.CBRegionCodes != undefined) {
                       var sel = this.CBRegionCodes.find(x => x.id == id);
                       if (sel != undefined) {
                           return sel.value;
                       }
                   }
                   return "";
               },
               _getFunctionalAreaDescription: function (id) {
                   if (id != undefined && this.CBFunctionalAreaCodes != undefined) {
                       var sel = this.CBFunctionalAreaCodes.find(x => x.id == id);
                       if (sel != undefined) {
                           return sel.value;
                       }
                   }
                   return "";
               },
               _getResponsabilityCenterDescription: function (id) {
                   if (id != undefined && this.CBResponsabilityCenterCodes != undefined) {
                       var sel = this.CBResponsabilityCenterCodes.find(x => x.id == id);
                       if (sel != undefined) {
                           return sel.value;
                       }
                   }
                   return "";
               },
               _responsabilityCenterCodeChanged: function () {
                    if (this.dataLines != null) {
                        var count = this.dataLines.length;
                        for (var i = 0; i < count; i++) {
                            this.dataLines[i].responsabilityCenterCode = this.$.ResponsabilityCenterCode.value;
                        }
                        this.$.grid_requisitionlines.clearCache();

                    }
               },
               _descriptionMeal: function (param) {
                   if (param != undefined && this.CBMealType != undefined) {
                       var sel = this.CBMealType.find(x => x.id == param);
                       if (sel != undefined) {
                           return sel.value;
                       }
                   }
                   return "";
               },

               _goBack: function (e) {
                   window.history.back();
               },
            });
        });
    </script>

</dom-module>
