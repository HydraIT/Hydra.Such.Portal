
@{
    if (ViewBag.Option == "historico")
    {
        ViewData["Title"] = "Histórico Requisições Simplificadas";
    }
    else if (ViewBag.Option == "registar")
    {
        ViewData["Title"] = "Requisições Simplificadas para Registar";
    }
    else
    {
        ViewData["Title"] = "Requisições Simplificadas";
    }
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<link rel="import" href="/lib/paper-tabs-master/paper-tab.html" />
<link rel="import" href="/lib/paper-tabs-master/paper-tabs.html" />
<link rel="import" href="/lib/iron-pages-master/iron-pages.html" />
<link rel="import" href="/lib/iron-menu-behavior/iron-menubar-behavior.html" />
<link rel="import" href="/lib/iron-form/iron-form.html" />
<link rel="import" href="/lib/paper-button/paper-button.html" />
<link rel="import" href="/lib/paper-input/paper-input.html" />
<link rel="import" href="/lib/paper-toast-master/paper-toast.html" />
<link rel="import" href="/lib/vaadin-valo-theme/vaadin-date-picker.html">
<link rel="import" href="~/lib/vaadin-date-picker/vaadin-date-picker.html">
<link rel="import" href="/lib/vaadin-valo-theme/vaadin-checkbox.html">
<link rel="import" href="/lib/vaadin-text-field/vaadin-text-field.html">
<link rel="import" href="/lib/vaadin-grid/vaadin-grid.html">
<link rel="import" href="/lib/vaadin-grid/vaadin-grid-sorter.html">
<link rel="import" href="/lib/vaadin-grid/vaadin-grid-filter.html">
<link rel="import" href="/lib/vaadin-checkbox/vaadin-checkbox.html">
<link rel="import" href="/lib/vaadin-combo-box/vaadin-combo-box.html" />
<link rel="import" href="/lib/paper-dialog/paper-dialog.html" />
<link rel="import" href="/lib/paper-dialog-behavior/paper-dialog-behavior.html" />
<link rel="import" href="/lib/vaadin-grid/vaadin-grid-selection-column.html" />
<link rel="import" href="/lib/vaadin-grid/vaadin-grid-active-item-behavior.html" />
<link rel="import" href="/lib/vaadin-grid/vaadin-grid-selection-behavior.html" />
<link rel="import" href="/lib/such-selectbox/such-selectbox.html">
<link rel="import" href="/lib/such-textarea/such-textarea.html">
<link rel="import" href="/lib/paper-collapse-item/paper-collapse-group.html">
<link rel="import" href="/lib/paper-collapse-item/paper-collapse-item.html">

<x-simplifiedreqdetails _requestno="@ViewBag.RequestNo" _permissions='@Html.Raw(Json.Serialize(ViewBag.UPermissions))'></x-simplifiedreqdetails>

<dom-module id="x-simplifiedreqdetails">
    <template>
        <!-- STYLES -->
        <link rel="stylesheet" href="/lib/bootstrap/dist/css/bootstrap.css" />
        <link rel="stylesheet" href="/lib/font-awesome-4.7.0/css/font-awesome.min.css" />
        <link rel="stylesheet" href="/css/main.css" />
        <style>
            vaadin-combo-box {
                margin-top: 0px !important;
                width: 100% !important;
            }

            vaadin-date-picker {
                margin-top: 0px !important;
                width: 100% !important;
            }

            vaadin-text-field {
                margin-top: 0px !important;
                width: 100%;
            }

            paper-button {
                margin-top: 10px;
            }

            .break_clear {
                margin-top: 15px;
                margin-right: 0px !important;
                margin-left: 0px !important;
            }

            .break_frm {
                margin-top: 15px;
            }

            .marginSpace {
                margin: 0px !important;
            }


            /*ACORDEÃO*/
            paper-collapse-item {
                --paper-item: {
                    padding: 0 0;
                };
                --paper-collapse-item-header: {
                    padding: 0 15px;
                    color: #333;
                    background-color: #f5f5f5;
                    border-bottom: solid #ddd 1px;
                    border-top-left-radius: 3px;
                    border-top-right-radius: 3px;

                };
            }
        </style>

        <!-- DROPDOWNS POPULATE -->
        <iron-ajax url="/PopulateDropdowns/GetFunctionalAreaCode" last-response="{{CBFunctionalAreaCodes}}" auto method="post" handle-as="json" content-type="application/json"></iron-ajax>
        <iron-ajax url="/PopulateDropdowns/GetResponsabilityCenterCode" last-response="{{CBResponsabilityCenterCodes}}" auto method="post" handle-as="json" content-type="application/json"></iron-ajax>
        <iron-ajax url="/PopulateDropdowns/GetRegionCode" last-response="{{CBRegionCodes}}" auto method="post" handle-as="json" content-type="application/json"></iron-ajax>
        <iron-ajax url="/PopulateDropdowns/GetLocations" last-response="{{CBLocations}}" auto method="post" handle-as="json" content-type="application/json"></iron-ajax>
        <iron-ajax url="/PopulateDropdowns/GetProjectList" last-response="{{CBProjects}}" auto method="post" handle-as="json" content-type="application/json"></iron-ajax>
        <iron-ajax url="/PopulateDropdowns/GetProjects" last-response="{{sbProjects}}" auto method="post" handle-as="json" content-type="application/json"></iron-ajax>
        <iron-ajax url="/PopulateDropdowns/GetMealTypes" last-response="{{CBMealType}}" auto method="post" handle-as="json" content-type="application/json"></iron-ajax>
        <iron-ajax url="/PopulateDropdowns/GetUnitStockeeping" last-response="{{CBProductos}}" auto method="post" handle-as="json" content-type="application/json"></iron-ajax>
        <iron-ajax url="/PopulateDropdowns/GetUnitOfMeasureByCode" last-response="{{CBMeasureUnits}}" auto method="post" handle-as="json" content-type="application/json"></iron-ajax>
        <iron-ajax url="/PopulateDropdowns/GetRequisitionsStatus" last-response="{{GetRequisitionsStatus}}" auto method="post" handle-as="json" content-type="application/json"></iron-ajax>
        <iron-ajax url="/PopulateDropdowns/GetEmployees" last-response="{{CBEmployees}}" auto method="post" handle-as="json" content-type="application/json"></iron-ajax>
        <!-- PROJECT CRUDS -->

        <iron-ajax url="/RequisicoesSimplificadas/GetSimplifiedRequisitionData"
                   last-response="{{result}}"
                   method="post"
                   handle-as="json"
                   id="page_databound_ajax"
                   on-response="_responseProcessor"
                   content-type="application/json"></iron-ajax>

        <iron-ajax url="/RequisicoesSimplificadas/GetSimplifiedRequisitionModel"
                   last-response="{{result}}"
                   method="post"
                   handle-as="json"
                   id="page_databound_ajax2"
                   on-response="_responseProcessor2"
                   content-type="application/json"></iron-ajax>

        <iron-ajax last-response="{{dataLines}}"
                   method="post"
                   handle-as="json"
                   id="updatemultiLine_ajax"
                   content-type="application/json"></iron-ajax>

        <iron-ajax last-response="{{requisitions}}"
                   method="post"
                   handle-as="json"
                   id="register_requisitions_ajax"
                   on-response="_responseProcessorRegister"
                   content-type="application/json"></iron-ajax>

        <iron-ajax last-response="{{dataLines}}"
                   method="post"
                   handle-as="json"
                   id="contractLines_databound_ajax"
                   on-response="_responseContractLines"
                   content-type="application/json"></iron-ajax>

        <iron-ajax last-response="{{updateLines}}"
                   method="post"
                   handle-as="json"
                   on-response="_responseProcessorLines"
                   id="update_lines_requisition_ajax"
                   content-type="application/json"></iron-ajax>

        <iron-ajax url="/RequisicoesSimplificadas/DeleteSimplifiedRequisition"
                   last-response="{{deletedData}}"
                   method="post"
                   handle-as="json"
                   id="ajax_delete_project"
                   on-response="_responseDeleteProcessor"
                   content-type="application/json"></iron-ajax>

        <iron-ajax url="/RequisicoesSimplificadas/DeleteSimplifiedRequisitionLines"
                   last-response="{{deletedData}}"
                   method="post"
                   handle-as="json"
                   id="ajax_delete_lines_project"
                   on-response="_responseDeleteLinesProcessor"
                   content-type="application/json"></iron-ajax>

        <iron-ajax last-response="{{resultModels}}"
                   method="post"
                   handle-as="json"
                   id="get_models_ajax"
                   on-response="_responseGetModels"
                   content-type="application/json"></iron-ajax>

        <iron-ajax url="/RequisicoesSimplificadas/ValidateNumeration"
                   last-response="{{validateResponse}}"
                   method="post"
                   handle-as="json"
                   id="page_validator_ajax"
                   on-response="_responseReqValidatorProcessor"
                   content-type="application/json"></iron-ajax>

        <template is="dom-if" if="{{ !_isCreate }}">
            <div class="row">
                <div class="col-lg-12">

                    <div class="subMenu">
                        <paper-button class="tablinks" on-tap="_goBack" title="Voltar"><i class="fa fa-arrow-left" aria-hidden="true"></i> Voltar</paper-button>

                        <template is="dom-if" if="{{ _permissions.create }}">
                            <paper-button class="tablinks" on-tap="_createNew"><i class="fa fa-plus-square" aria-hidden="true"></i> Nova Requisição</paper-button>
                        </template>
                        <template is="dom-if" if="{{ _permissions.update }}">
                            <paper-button class="tablinks" on-tap="_finishRequisition"><i class="fa fa-check-square" aria-hidden="true"></i> Terminar Requisição</paper-button>
                        </template>
                    </div>

                </div>
            </div>

        </template>
        <template is="dom-if" if="{{ _isCreate }}">
            <div class="row">
                <div class="col-lg-12">
                    <template is="dom-if" if="{{ _permissions.create }}">
                        <div class="subMenu">
                            <paper-button class="tablinks" on-tap="_copyModel"><i class="fa fa-clone" aria-hidden="true"></i> Copiar Requisição de Modelo</paper-button>
                            <paper-button class="tablinks" on-tap="_copyHistory"><i class="fa fa-clone" aria-hidden="true"></i> Copiar Requisição de Histórico</paper-button>
                        </div>
                    </template>
                </div>
            </div>

        </template>
        &nbsp;
        @* Botões de registar requisições aprovadas*@
        <div class="subMenu" id="ddsubMenu">
            <paper-button class="tablinks" on-tap="_registeRequisiApproval"><i class="fa fa-chevron-circle-down" aria-hidden="true"></i> Registar Linhas de Diário</paper-button>
            <paper-button class="tablinks" on-tap="_desaprovarRequisiApproval"><i class="fa fa-check-square" aria-hidden="true"></i> Desaprovar Requisições</paper-button>
            <paper-button class="tablinks" on-tap="_historyRequisiApproval"><i class="fa fa-check-square" aria-hidden="true"></i> Arquivar Requisições</paper-button>
        </div>
        &nbsp;
        <div class="row break_frm">
            <div class="col-md-12 col-lg-12">
                <div class="panel panel-default">
                    <div class="panel-heading">Cabeçalho</div>
                    <div class="panel-body">
                        <iron-form id="formContract">
                            <paper-tabs fit-container no-slide autoselect autoselect-delay="0" selected="{{selected}}">
                                <paper-tab>Geral</paper-tab>
                                <paper-tab>Detalhe</paper-tab>
                            </paper-tabs>
                            <iron-pages selected="{{selected}}">
                                <!--Geral-->
                                <div>
                                    <div class="row break_clear">
                                        <div class="col-lg-3">
                                            <vaadin-text-field value="{{_requestno}}" maxlength="20" id="lRequestNo" name="lRequestNo" label="Nº" required error-message="Este campo é obrigatório" disabled> </vaadin-text-field>
                                        </div>
                                        <div class="col-lg-3">
                                            <vaadin-combo-box items="[[GetRequisitionsStatus]]" id="LStatus" selected="1" item-value-path="id" item-label-path="value" value="{{result.status}}" label="Estado" disabled>
                                                <template>
                                                    <b>[[item.value]]</b>
                                                </template>
                                            </vaadin-combo-box>
                                        </div>
                                        <div class="col-lg-3 alignPoga">
                                            <vaadin-checkbox checked="{{result.finished}}" disabled>Terminada</vaadin-checkbox>
                                        </div>
                                        <div class="col-lg-3">
                                            <vaadin-combo-box items="[[CBLocations]]" id="LocationCode" item-value-path="id" item-label-path="value" value="{{result.locationCode}}" label="Cód. Localização" disabled="{{ !_permissions.update }}" on-value-changed="_locationCodeChanged">
                                                <template>
                                                    <b>[[item.id]]</b>
                                                    <b>[[item.value]]</b>
                                                </template>
                                            </vaadin-combo-box>
                                        </div>
                                    </div>
                                    <div class="row break_clear">
                                        <div class="col-lg-3">
                                            <such-selectbox id="ProjectNo" name="ProjectNo" value="{{result.projectNo}}" label="Nº Projeto" on-value-changed="_projectoNoChanged" disabled="@ViewBag.LockFields"
                                                            items="[[sbProjects]]"
                                                            text-path="nºProjeto"
                                                            value-path="nºProjeto"
                                                            fields='[{"field":"nºProjeto", "name":"Código"}, {"field":"descrição", "name":"Descrição"}, {"field":"nºCliente", "name":"Nº Cliente"}]'>
                                            </such-selectbox>
                                        </div>
                                        <div class="col-lg-3">
                                            <vaadin-combo-box items="[[CBEmployees]]" id="LEmployeeNo" name="LEmployeeNo" item-value-path="id" item-label-path="value" value="{{result.employeeNo}}" label="Funcionário" disabled="{{ !_permissions.create }}">
                                                <template>
                                                    <paper-icon-item>
                                                        <paper-item-body two-line>
                                                            [[item.id]]
                                                            <b>[[item.value]]</b>
                                                        </paper-item-body>
                                                    </paper-icon-item>
                                                </template>
                                            </vaadin-combo-box>
                                        </div>
                                        <div class="col-lg-3">
                                            <vaadin-combo-box items="[[CBMealType]]" id="LMealType" item-value-path="id" item-label-path="value" value="{{result.mealType}}" on-value-changed="_mealTypeChanged" label="Tipo Refeição" disabled="{{ !_permissions.create }}">
                                                <template>
                                                    <b>[[item.id]]</b>
                                                    <b>[[item.value]]</b>
                                                </template>
                                            </vaadin-combo-box>
                                        </div>
                                        <div class="col-lg-3">
                                            <vaadin-text-field id="LTotalCost" name="LTotalCost" label="Custo Total" disabled> </vaadin-text-field>
                                        </div>
                                    </div>
                                    <div class="row break_clear">
                                        <div class="col-lg-3">
                                            <vaadin-combo-box items="[[CBRegionCodes]]" id="RegionCode" item-value-path="id" item-label-path="value" value="{{result.regionCode}}" label="Cód. Região" disabled="{{ !_permissions.create }}" on-value-changed="_regionCodeChanged">
                                                <template>
                                                    <table width="180px">
                                                        <tr>
                                                            <td width="20%">[[item.id]]</td>
                                                            <td width="80%"> <b>[[item.value]]</b></td>
                                                        </tr>
                                                    </table>
                                                </template>
                                            </vaadin-combo-box>
                                        </div>
                                        <div class="col-lg-3">
                                            <vaadin-combo-box items="[[CBFunctionalAreaCodes]]" id="FunctionalAreaCode" item-value-path="id" item-label-path="value" value="{{result.functionalAreaCode}}" label="Cód. Área Funcional" disabled="{{ !_permissions.create }}" on-value-changed="_areaCodeChanged">
                                                <template>
                                                    <table width="500px">
                                                        <tr>
                                                            <td width="10%">[[item.id]]</td>
                                                            <td width="90%"> <b>[[item.value]]</b></td>
                                                        </tr>
                                                    </table>
                                                </template>
                                            </vaadin-combo-box>
                                        </div>
                                        <div class="col-lg-3">
                                            <vaadin-combo-box items="[[CBResponsabilityCenterCodes]]" id="ResponsabilityCenterCode" item-value-path="id" item-label-path="value" value="{{result.responsabilityCenterCode}}" label="Cód. Centro Responsabilidade" disabled="{{ !_permissions.create }}" on-value-changed="_responsabilityCenterCodeChanged">
                                                <template>
                                                    <table width="550px">
                                                        <tr>
                                                            <td width="20%">[[item.id]]</td>
                                                            <td width="80%"> <b>[[item.value]]</b></td>
                                                        </tr>
                                                    </table>
                                                </template>
                                            </vaadin-combo-box>
                                        </div>
                                        <div class="col-lg-3 ">
                                            <vaadin-date-picker label="Data Receção" id="LReceiptPreviewDate" name="LReceiptPreviewDate" value="{{result.receiptPreviewDate}}" disabled="{{ !_permissions.create }}"></vaadin-date-picker>
                                        </div>

                                    </div>
                                    <div class="row break_clear">
                                        <div class="col-lg-12">
                                            <such-textarea value="{{result.observations}}" rows="6" id="LObservations" name="LObservations" label="Observações" disabled="{{ !_permissions.create }}"> </such-textarea>
                                        </div>
                                    </div>
                                </div>
                                <!--Detalhe-->
                                <div>
                                    <div class="row break_clear">
                                        <div class="col-lg-3">
                                            <vaadin-text-field value="{{result.createResponsible}}" id="LCreateResponsible" name="LCreateResponsible" label="Responsável Criação" disabled> </vaadin-text-field>
                                        </div>
                                        <div class="col-lg-3">
                                            <vaadin-text-field value="{{result.requisitionDate}}" id="LRequisitionDate" name="LRequisitionDate" label="Data Requisição" disabled> </vaadin-text-field>
                                        </div>
                                        <div class="col-lg-3">
                                            <vaadin-text-field value="{{result.requisitionTime}}" id="LRequisitionTime" name="LRequisitionTime" label="Hora Requisição" disabled> </vaadin-text-field>
                                        </div>
                                        <div class="col-lg-3">
                                            <vaadin-text-field value="{{result.approvalResponsible}}" id="LApprovalResponsible" name="LApprovalResponsible" label="Responsável Aprovação" disabled> </vaadin-text-field>
                                        </div>
                                    </div>
                                    <br />
                                    <div class="row break_clear">
                                        <div class="col-lg-3">
                                            <vaadin-text-field value="{{result.approvalDate}}" id="LApprovalDate" name="LApprovalResponsible" label="Data Aprovação" disabled> </vaadin-text-field>
                                        </div>
                                        <div class="col-lg-3">
                                            <vaadin-text-field value="{{result.registrationDate}}" id="LApprovalDate" name="LApprovalResponsible" label="Data Registo" disabled> </vaadin-text-field>
                                        </div>
                                    </div>
                                </div>
                            </iron-pages>
                            <div class="marginSpace" id="ddmarginSpaceCabe">
                                <template is="dom-if" if="{{ _isCreate }}">
                                    <paper-button raised id="btnCreate" class="btnSave" on-tap="_create"><i class="fa fa-plus" aria-hidden="true"></i>&nbsp; Criar</paper-button>
                                </template>
                                <template is="dom-if" if="{{ !_isCreate }}">
                                    <template is="dom-if" if="{{ _permissions.update }}">
                                        <paper-button raised id="btnUpdate" class="btnSave" on-tap="_update"><i class="fa fa-floppy-o" aria-hidden="true"></i>&nbsp; Guardar</paper-button>
                                    </template>
                                    <template is="dom-if" if="{{ _permissions.delete }}">
                                        <paper-button raised id="btnCancel" class="btnRemove" on-tap="_delete"><i class="fa fa-trash-o" aria-hidden="true"></i>&nbsp; Eliminar</paper-button>
                                    </template>
                                </template>
                            </div>
                        </iron-form>
                    </div>
                    <paper-collapse-group style="display: {{ _showLinesForm }};">
                        <paper-collapse-item class="panel-heading" header="Linhas" closed>
                            <iron-form id="formcontractLines">
                                <div class="row break_clear">
                                    <div class="col-lg-2">
                                        <vaadin-combo-box items="[[CBLocations]]" id="_CLLocationCode" name="_LLLocation" item-value-path="id" item-label-path="value" value="{{_LLLocation}}" label="Localização" disabled="{{ !_permissions.update }}" required error-message="Este campo é obrigatório">
                                            <template>
                                                <b>[[item.value]]</b>
                                            </template>
                                        </vaadin-combo-box>
                                    </div>
                                    <div class="col-lg-2">
                                        <such-selectbox id="_CLCode" value="{{_LLProduct}}" label="Cód. Produto"
                                                        items="[[CBProductos]]"
                                                        text-path="id"
                                                        value-path="id"
                                                        fields='[{"field":"id", "name":"Cód. Produto"}, {"field":"value", "name":"Descrição"}, {"field":"extra", "name":"Custo Unitário"}, {"field":"extra2", "name":"Unidade Medida"}]'
                                                        disabled="{{ !_permissions.update }}"
                                                        on-value-changed="_productChanged" required error-message="Este campo é obrigatório">
                                        </such-selectbox>
                                    </div>
                                    <div class="col-lg-2">
                                        <vaadin-text-field value="{{_LLDescription}}" id="_CLDescription" name="_LLDescription" label="Descrição" disabled="{{ !_permissions.update }}"> </vaadin-text-field>
                                    </div>
                                    <div class="col-lg-2">
                                        <vaadin-text-field value="{{_LLUnitCost}}" type="number" id="_CLUnitCost" name="_LLDescription" label="Preço Unico" disabled>  €</vaadin-text-field>
                                    </div>
                                    <div class="col-lg-2">
                                        <vaadin-combo-box items="[[CBMeasureUnits]]" id="_CLMeasureUnitNo" name="_LLMeasureUnit" item-value-path="id" item-label-path="value" value="{{_LLMeasureUnit}}" label="Cód. Unidade Medida" disabled>
                                            <template>
                                                <b>[[item.value]]</b>
                                            </template>
                                        </vaadin-combo-box>
                                    </div>
                                    <div class="col-lg-2">
                                        <vaadin-text-field value="{{_LLQuantityToRequire}}" type="number" id="_CLQuantityToRequire" name="_LLQuantityToRequire" label="Quantidade a requerer" disabled="{{ !_permissions.update }}" on-value-changed="_readQuantity" required error-message="Este campo é obrigatório"> </vaadin-text-field>
                                    </div>
                                </div>
                                <div class="row break_clear">
                                    <div class="col-lg-2">
                                        <vaadin-text-field value="{{_LLTotalCost}} " id="_CLTotalCost" name="_LLTotalCost" label="Custo Total" disabled> </vaadin-text-field>
                                    </div>
                                    <div class="col-lg-2">
                                        <such-selectbox id="_CLProjectNo" name="_CLProjectNo" value="{{result.projectNo}}" label="Nº Projeto" disabled="{{ _lockFields }}"
                                                        items="[[sbProjects]]"
                                                        text-path="nºProjeto"
                                                        value-path="nºProjeto"
                                                        fields='[{"field":"nºProjeto", "name":"Código"}, {"field":"descrição", "name":"Descrição"}, {"field":"nºCliente", "name":"Nº Cliente"}]'>
                                        </such-selectbox>
                                    </div>
                                    <div class="col-lg-2">
                                        <vaadin-combo-box items="[[CBMealType]]" id="_CLMealType" item-value-path="id" item-label-path="value" value="{{result.mealType}}" label="Tipo Refeição" disabled="{{ _lockFields }}">
                                            <template>
                                                <b>[[item.value]]</b>
                                            </template>
                                        </vaadin-combo-box>
                                    </div>
                                    <div class="col-lg-2">
                                        <vaadin-date-picker label="Data Registo" id="_CLRegistrationDate" name="_CLRegistrationDate" value="{{result.receiptPreviewDate}}" disabled="{{ _lockFields }}"></vaadin-date-picker>
                                    </div>
                                    <div class="col-lg-2">
                                        <vaadin-combo-box items="[[CBRegionCodes]]" id="_CLRegionCode" name="_CLRegion" item-value-path="id" item-label-path="value" value="{{result.regionCode}}" label="Cód. Região" disabled="{{ _lockFields }}">
                                            <template>
                                                <table width="180px">
                                                    <tr>
                                                        <td width="20%">[[item.id]]</td>
                                                        <td width="80%"> <b>[[item.value]]</b></td>
                                                    </tr>
                                                </table>
                                            </template>
                                        </vaadin-combo-box>
                                    </div>
                                    <div class="col-lg-2">
                                        <vaadin-combo-box items="[[CBFunctionalAreaCodes]]" id="_CLFunctionalArea" name="_CLFunctionalArea" item-value-path="id" item-label-path="value" value="{{result.functionalAreaCode}}" label="Cód. Área Funcional" disabled="{{ _lockFields }}">
                                            <template>
                                                <table width="500px">
                                                    <tr>
                                                        <td width="10%">[[item.id]]</td>
                                                        <td width="90%"> <b>[[item.value]]</b></td>
                                                    </tr>
                                                </table>
                                            </template>
                                        </vaadin-combo-box>
                                    </div>
                                </div>
