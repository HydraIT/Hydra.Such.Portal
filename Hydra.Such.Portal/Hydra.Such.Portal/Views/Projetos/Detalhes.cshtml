@{
    ViewData["Title"] = ViewBag.ProjectNo == "" ? "Criar Projeto" : "Detalhes de Projeto";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<link rel="import" href="/elements/projects/project_details.html">

<x-projectdetails _projectid="@ViewBag.ProjectNo"
                  _areaid="1"
                  _permissions='@Html.Raw(Json.Serialize(ViewBag.UPermissions))'
                  _reportserverurl='@ViewBag.reportServerURL'
                  _projectclosed='@ViewBag.projectClosed'>

</x-projectdetails>
