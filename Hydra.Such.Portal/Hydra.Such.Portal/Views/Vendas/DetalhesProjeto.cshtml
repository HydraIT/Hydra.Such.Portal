
@{
    ViewData["Title"] = "Detalhes Projeto - Vendas - Such";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@if (ViewBag.ProjectNo == "")
{
    <h2>Criar novo projeto</h2>
}
else
{
    <h2>Projeto @ViewBag.ProjectNo</h2>
}


<link rel="import" href="/elements/projects/project_details.html">

<x-projectdetails _projectid="@ViewBag.ProjectNo" _areaid="4" _permissions='@Html.Raw(Json.Serialize(ViewBag.UPermissions))'></x-projectdetails>
