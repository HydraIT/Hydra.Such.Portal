
@{
    ViewData["Title"] = "Detalhes Contrato - Engenharia";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<link rel="import" href="~/elements/contracts/contract_details.html">

@if (ViewBag.ContractNo == "")
{
    <h2>Criar novo contrato</h2>
}
else
{
    <h1 class="title">Contrato Engenharia @ViewBag.ContractNo</h1>
}


<x-contractdetails _contractno="@ViewBag.ContractNo" _versionno="@ViewBag.VersionNo" _areaid="1" _permissions='@Html.Raw(Json.Serialize(ViewBag.UPermissions))'></x-contractdetails>


