
@{
    ViewData["Title"] = "DetalhesOportunidade";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<link rel="import" href="~/elements/oportunities/oportunity_details.html">
@if (ViewBag.ContractNo == "")
{
    <h2>Criar Nova Oportunidade</h2>
}
else
{
    <h1 class="title">Oportunidade Internacionalizações @ViewBag.ContractNo</h1>
}


<x-oportunitydetails _contractno="@ViewBag.ContractNo" _versionno="@ViewBag.VersionNo" _areaid="8" _permissions='@Html.Raw(Json.Serialize(ViewBag.UPermissions))'></x-oportunitydetails>