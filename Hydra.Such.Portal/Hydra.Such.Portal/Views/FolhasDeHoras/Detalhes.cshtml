@{
    @if (ViewBag.FolhaDeHorasNo == "")
    {
        ViewData["Title"] = "Criar nova Folha de Horas";
    }
    else
    {
        if (ViewBag.Estado == 2)
        {
            ViewData["Title"] = "Folha de Horas (Histórico) > Nº " + ViewBag.FolhaDeHorasNo;
        }
        else
        {
            ViewData["Title"] = "Folha de Horas > Nº " + ViewBag.FolhaDeHorasNo;
        }
    }

        Layout = "~/Views/Shared/_Layout.cshtml";
    }

<link rel="import" href="/elements/folhasdehoras/folhadehora_details.html">

<x-folhadehoradetails _folhadehoraid='@ViewBag.FolhaDeHorasNo'
                      _areaid='1'
                      _reportserverurl='@ViewBag.reportServerURL'
                      _userlogin='@ViewBag.userLogin'
                      _permissions='@Html.Raw(Json.Serialize(ViewBag.UPermissions))'>
</x-folhadehoradetails>
